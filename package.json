  "bugs": {
    "url": "https://github.com/LooseLi/wx-lala/issues"
  },
  "homepage": "https://github.com/LooseLi/wx-lala#readme",
  "devDependencies": {
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-miniprogram": "^0.2.0",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3"
  }
